<% content_for :title do %>422 Rejected!<% end %>

<% content_for :content do %>
	<article class="info">
		<h2>422 Rejected!</h2>
	</article>

    <p>The change you wanted was rejected. Maybe you tried to change something you didn't have access to.</p>

	<% @posts.each do |post| %>
		<%= render :partial => "layouts/post", :locals => { :post => post, :format => :large, :links => 'public' } %>
	<% end %>
<% end %>